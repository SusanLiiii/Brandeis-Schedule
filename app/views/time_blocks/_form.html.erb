<%= form_with(model: time_block) do |form| %>
  <% if time_block.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(time_block.errors.count, "error") %> prohibited this time_block from being saved:</h2>

      <ul>
        <% time_block.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
