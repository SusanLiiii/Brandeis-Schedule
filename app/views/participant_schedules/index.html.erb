<p id="notice"><%= notice %></p>

<h1>Participant Schedules</h1>

<table>
  <thead>
    <tr>
      <th>Participant</th>
      <th>Event</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @participant_schedules.each do |participant_schedule| %>
      <tr>
        <td><%= participant_schedule.participant_id %></td>
        <td><%= participant_schedule.event_id %></td>
        <td><%= link_to 'Show', participant_schedule %></td>
        <td><%= link_to 'Edit', edit_participant_schedule_path(participant_schedule) %></td>
        <td><%= link_to 'Destroy', participant_schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Participant Schedule', new_participant_schedule_path %>
